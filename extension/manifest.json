{
	"manifest_version": 2,

	"name": "WordBird",
	"description": "Change from Word to Word",
	"version": "0.1",

	"permissions": [
		"activeTab",
		 "storage"
	],

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup/popup.html"
	},

	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["content_script.js"],
		"run_at": "document_end"
	}]
}
